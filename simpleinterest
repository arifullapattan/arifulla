def simple_interest(p,t,r):
    print("the pricipal is",p)
    print("the time period",t)
    print("the rate of interest is",r)
    if r >= 12
    print("senior citizen")
    else r < 12
    print("not a senior citizen")
    si = (p * t * r)/100
    print("the simple interest is",si)
    return si
